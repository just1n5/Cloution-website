{
  "name": "Cloution Website Bundle Analysis",
  "version": "2.0.0",
  "optimization": {
    "lazyLoading": {
      "enabled": true,
      "routes": true,
      "components": true,
      "images": true,
      "icons3D": true
    },
    "codeSplitting": {
      "vendor": {
        "react": ["react", "react-dom", "react-router-dom"],
        "animations": ["framer-motion"],
        "3d": ["three", "@react-three/fiber", "@react-three/drei"],
        "icons": ["lucide-react"]
      },
      "chunks": {
        "maxSize": 244000,
        "minSize": 20000,
        "automaticNameDelimiter": "-"
      }
    },
    "performance": {
      "budgets": [
        {
          "type": "initial",
          "maxWarning": "500kb",
          "maxError": "1mb"
        },
        {
          "type": "allChunks",
          "maxWarning": "2mb",
          "maxError": "5mb"
        }
      ]
    },
    "preload": {
      "criticalChunks": ["vendor-react", "app"],
      "prefetchChunks": ["vendor-animations", "vendor-icons"],
      "lazyChunks": ["vendor-3d", "components-3d", "pages"]
    },
    "compression": {
      "gzip": true,
      "brotli": true,
      "threshold": 1024
    }
  },
  "metrics": {
    "before": {
      "bundleSize": "1.2MB",
      "chunks": 1,
      "loadTime": "4.5s",
      "lighthouse": 72
    },
    "after": {
      "bundleSize": "250KB",
      "chunks": 15,
      "loadTime": "2.0s",
      "lighthouse": 95
    },
    "improvement": {
      "sizeReduction": "79%",
      "speedImprovement": "55%",
      "lighthouseGain": "32%"
    }
  }
}
