# Variables de entorno de ejemplo
# Copia este archivo a .env.local y configura tus propias variables

# API URLs
VITE_API_URL=https://api.cloution.com
VITE_API_KEY=your_api_key_here

# Analytics
VITE_GA_TRACKING_ID=UA-XXXXXXXXX-X
VITE_GTM_ID=GTM-XXXXXX

# Contact Form
VITE_EMAILJS_SERVICE_ID=service_xxxxx
VITE_EMAILJS_TEMPLATE_ID=template_xxxxx
VITE_EMAILJS_PUBLIC_KEY=xxxxxxxxxxxxx

# Social Media
VITE_FACEBOOK_URL=https://facebook.com/cloution
VITE_TWITTER_URL=https://twitter.com/cloution
VITE_LINKEDIN_URL=https://linkedin.com/company/cloution
VITE_INSTAGRAM_URL=https://instagram.com/cloution
VITE_GITHUB_URL=https://github.com/cloution

# Features Flags
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_CHAT=false
VITE_ENABLE_NEWSLETTER=true
