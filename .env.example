# Variables de Entorno - Cloution Website
# Copiar este archivo como .env.local para desarrollo

# API URLs
VITE_API_URL=https://api.cloution.com
VITE_GOOGLE_FORMS_URL=https://docs.google.com/forms/d/e/1FAIpQLSdNR8LdWXRiSNCdBj4iJacptpNQRXt4MfVXJTYq7HNpleclJA/formResponse

# Google Analytics (opcional)
VITE_GA_TRACKING_ID=UA-XXXXXXXXX-X

# Configuración de Features
VITE_ENABLE_3D=true
VITE_ENABLE_ANIMATIONS=true
VITE_DEBUG_MODE=false

# Seguridad
VITE_ENCRYPTION_KEY=cloution-2024-secure-key
VITE_MAX_FORM_ATTEMPTS=3
VITE_RATE_LIMIT_WINDOW=60000

# Firebase Config (se agregará después de crear el proyecto)
